        "                    zins_zahlung_durchscnitt += monatliche_zins_zahlung\n",
        "                    #print(\"Zinsen: \" + str(round(monatliche_zins_zahlung,2)))\n",
        "                    self.monatlicher_betrag = (((((self.imo_preis + self.kaufnebenkosten_in_euro)-self.eigenkapital) * self.zins_in_prozent) / 100) / 12) + (((((self.imo_preis + self.kaufnebenkosten_in_euro)-self.eigenkapital) * self.tilgung_in_prozent) / 100) / 12)\n",
        "                    monatliche_tilgung = self.monatlicher_betrag - monatliche_zins_zahlung\n",
        "                    #print(\"Tilgung: \" + str(monatliche_tilgung))\n",
        "                    for _ in range(12):\n",
        "                        zu_tilgender_betrag = (zu_tilgender_betrag - monatliche_tilgung)\n",
        "                else:\n",
        "                    inhalt = f\"Der Kredit ist nach ca. {jahre} Jahren abbezahlt\"\n",
        "                    zins_zahlung_durchscnitt /= jahre\n",
